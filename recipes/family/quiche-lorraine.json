{
    "name": "Quiche Lorraine",
    "image": "",
    "ingredients": [
        {
            "name": "Pie Crust - 9 inch",
            "quantity": "1",
            "unit": "none"
        },
        {
            "name": "Bacon",
            "quantity": "6",
            "unit": "piece"
        },
        {
            "name": "Onion",
            "quantity": "1/2",
            "unit": "cup"
        },
        {
            "name": "Shredded Swiss Cheese",
            "quantity": "1 1/2",
            "unit": "cup"
        },
        {
            "name": "Evaporated Milk",
            "quantity": "12",
            "unit": "oz"
        },
        {
            "name": "Eggs",
            "quantity": "3",
            "unit": "none"
        },
        {
            "name": "Salt",
            "quantity": "1/4",
            "unit": "tsp"
        },
        {
            "name": "Pepper",
            "quantity": "1/8",
            "unit": "tsp"
        },
        {
            "name": "Ground Nutmeg",
            "quantity": "1/8",
            "unit": "tsp"
        }
    ],
    "steps": [
        {
            "step": "Chop Onion. Cook Bacon in a large skillet over medium heat. When Bacon starts to brown, add Onion to skillet. Cook until Bacon is crispy and drain"
        },
        {
            "step": "Add Pie Crust to a baking dish. Add Shredded Swiss Cheese to the bottom of the Pie Crust. Add Bacon and Onion to the Pie Crust"
        },
        {
            "step": "Mix Milk, Eggs, Salt, Pepper, and Ground Nutmeg in a small bowl. Pour mixture into Pie Crust"
        },
        {
            "step": "Bake at 350F for 30-35 minutes or until a toothpick comes out of center clean"
        }
    ]
}