{
    "name": "Oatmeal Raisin Cookies",
    "image": "",
    "ingredients": [
        {
            "name": "Butter",
            "quantity": "14",
            "unit": "tbsp"
        },
        {
            "name": "Brown Sugar",
            "quantity": "3/4",
            "unit": "cup"
        },
        {
            "name": "Sugar",
            "quantity": "1/2",
            "unit": "cup"
        },
        {
            "name": "Eggs",
            "quantity": "2",
            "unit": "none"
        },
        {
            "name": "Vanilla",
            "quantity": "1",
            "unit": "tsp"
        },
        {
            "name": "Flour",
            "quantity": "1 1/2",
            "unit": "cup"
        },
        {
            "name": "Baking Soda",
            "quantity": "1",
            "unit": "tsp"
        },
        {
            "name": "Ground Cinnamon",
            "quantity": "1",
            "unit": "tsp"
        },
        {
            "name": "Salt",
            "quantity": "1/2",
            "unit": "tsp"
        },
        {
            "name": "Oats",
            "quantity": "3",
            "unit": "cup"
        },
        {
            "name": "Raisins",
            "quantity": "1",
            "unit": "cup"
        }
    ],
    "steps": [
        {
            "step": "In a large bowl, beat softened Butter, Sugar, and Brown Sugar until creamy"
        },
        {
            "step": "Add Eggs and Vanilla to mixture. Mix thoroughly"
        },
        {
            "step": "Add Flour, Baking Soda, Cinnamon, and Salt to mixture. Mix thoroughly"
        },
        {
            "step": "Add Oats and Raisins. Mix thoroughly"
        },
        {
            "step": "Spoon cookies onto an ungreased baking sheet. Bake at 350F for 8-10 minutes until light golden brown"
        }
    ]
}